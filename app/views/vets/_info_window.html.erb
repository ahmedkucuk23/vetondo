<%= link_to vet_path(vet), class: "card_link_1" do %>
  <div class="card_div" style="margin-bottom: 0;">
    <%= cl_image_tag vet.photo.key, height: 180, width: 180, crop: :fill, :class => "vet_prof_picture_info" %>
    <p class="card_info_name"> <strong> <%= vet.name %> </strong> </p>
    <p class="card_adress_info"> <%= vet.address %> </p>
    <p class="card_price"><%= vet.price %> BAM</p>
  </div>
<% end %>
